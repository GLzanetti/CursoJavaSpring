package Streams.DesafioFilter;

public class Canditados {
	
	final String nome;
	final int idade;
	final String sexo;
	final int experiencia;
	
	public Canditados(String nome, int idade, String sexo, int experiencia) {
		this.nome = nome;
		this.idade = idade;
		this.sexo = sexo;
		this.experiencia = experiencia;
	}
	
	
}
